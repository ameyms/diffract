{
  "name": "diffract",
  "author": "Amey Sakhadeo",
  "version": "0.0.1",
  "description": "A set of d3 based visualization components built for React",
  "main": "./lib",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf lib",
    "build:js": "jsx --harmony ./src ./lib",
    "build:jsx": "jsx --harmony -x jsx ./src ./lib",
    "build": "npm run build:js && npm run build:jsx",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "d3",
    "visualization",
    "graphs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ameyms/diffract.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ameyms/diffract/issues"
  },
  "dependencies": {
    "react-classset": "0.0.2"
  },
  "peerDependencies": {
    "react": "0.12.x",
    "d3": "^3.5.5"
  },
  "devDependencies": {
    "react-tools": "^0.12.2",
    "browserify": "^7.0.3",
    "reactify": "^0.17.1",
    "react-router": "^0.11.6"
  }
}
